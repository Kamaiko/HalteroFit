{
  "expo": {
    "name": "Halterofit",
    "slug": "halterofit",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "scheme": "halterofit",
    "backgroundColor": "#0A0A0A",
    "newArchEnabled": true,
    "plugins": ["expo-router", "@sentry/react-native", "expo-asset"],
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.halterofit.app",
      "adaptiveIcon": {
        "backgroundColor": "#0A0A0A"
      }
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.halterofit.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dc6006da-2abd-41a0-9b8b-32adc6f9bf3a"
      }
    }
  }
}
